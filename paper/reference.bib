  author = {Elise, Simon and Guilhaumon, Fran{\c c}ois and {Mou-Tham}, G{\'e}rard and {Urbina-Barreto}, Isabel and Vigliola, Laurent and Kulbicki, Michel and Bruggemann, J. Henrich},
